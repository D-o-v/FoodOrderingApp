{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "typeRoots": ["./node_modules/@types", "./src/types","./node_modules/typscript","/node_modules/type-is","/node_modules/ts-node"],
      "types": ["express", "node"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }


  